{{!-- {{log model.transactionsgroupedbydate}} --}}

<br>

<table>

<thead style="background-color: darkred;color: yellow;text-align: left;">
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Amount</th>
   </tr>
  </thead>

  <tbody>
    {{#each model.transactionsgroupedbynamedate as |namedate|}}
      {{#each namedate as |name|}}
        {{#each name as |nameval|}}
          {{#each nameval as |val|}}
            {{#each val as |value|}}
              <tr style="background-color: wheat;">
                <td>{{value.name}}</td>
                <td>{{value.date}}</td>
                <td>{{value.amount}}</td>
              </tr>
            {{/each}}
          {{/each}}
        {{/each}}
      {{/each}}
    {{/each}}
  </tbody>
</table>